    <div class="profile">
        <div class="left">
            <div class="prof">
                <div class="img">
                    <img src='<%= data.userData.profileImage %>' id="pht">
                </div>
                <div class="hd">
                    <h2>
                        <%= data.userData.name %>
                    </h2>
                </div>
            </div>
            <div class="details">
                <div class="same">
                    <div class="sm">
                        <h5>First Name</h5>
                        <p>
                            <%= data.userData.firstName %>
                        </p>
                    </div>
                    <div class="sm">
                        <h5>Last Name</h5>
                        <p>
                            <%= data.userData.lastName %>
                        </p>
                    </div>
                </div>
                <div class="same">
                    <div class="sm">
                        <h5>Email</h5>
                        <p>
                            <%= data.userData.email %>
                        </p>
                    </div>
                    <div class="sm">
                        <h5>Phn_No</h5>
                        <p>
                            <%= data.userData.phone %>
                        </p>
                    </div>
                </div>
                <div class="same">
                    <div class="sm">
                        <h5>Password</h5>
                        <p>*********</p>
                    </div>
                </div>
                <div class="same">
                    <a href="#">Change password</a>
                </div>
            </div>
        </div>
        <div class="right">
            <div class="righttop">
                <h3>Your Shopify Stores:</h3>
                <div class="det">
                    <h5>Shopify Store Name:</h5>
                    <p>
                        <%= data.storeData.shopifyStores[0].shopName %>
                    </p>
                </div>
                <div class="det">
                    <h5>Shopify Store URL:</h5>
                    <p>
                        <%= data.storeData.shopifyStores[0].shopifyStoreURL %>
                    </p>
                </div>
                <div class="det">
                    <a href="#">Orders</a>
                </div>
            </div>
            <div class="rightbot">
                <h3>Your WooCommerce Stores:</h3>
                <div class="det">
                    <h5>WooCommerce Store Name:</h5>
                    <p>
                        <%= data.storeData.wooCommerceStores[0].shopName %>
                    </p>
                </div>
                <div class="det">
                    <h5>WooCommerce Store URL:</h5>
                    <p>
                        <%= data.storeData.wooCommerceStores[0].url %>
                    </p>
                </div>
                <div class="det">
                    <a href="#">Orders</a>
                </div>
            </div>
        </div>
    </div>
